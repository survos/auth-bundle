{
    "name": "survos/auth-bundle",
    "description": "Helper commands and classes for symfony security",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "doctrine/orm": "^2.12 || ^3.3",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "symfony/config": "^7.3||^8.0",
        "symfony/console": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "symfony/options-resolver": "^7.3||^8.0",
        "symfony/password-hasher": "^7.3||^8.0",
        "symfony/security-bundle": "^7.3||^8.0",
        "symfony/yaml": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "survos/installer": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/ux-twig-component": "^2.8",
        "doctrine/doctrine-bundle": "^2.7|^3.0",
        "stevenmaguire/oauth2-dropbox": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Survos\\AuthBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "survos/installer": true
        }
    },
    "prefer-stable": true
}
